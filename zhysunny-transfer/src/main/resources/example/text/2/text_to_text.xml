<?xml version="1.0" encoding="UTF-8"?>

<!-- Field标签属性 -->
<!-- name属性表示输出text列名或者列索引 -->
<!-- value属性表示输入text列名或者列索引 -->
<!-- 注意text文本输出都用string，这里type属性无效 -->
<Mapping>
    <Field name="NAME" value="0"></Field>
    <Field name="AGE" value="1"></Field>
    <Field name="SEX" value="2"></Field>
</Mapping>
