<?xml version="1.0" encoding="UTF-8"?>
<!-- xml结构较多，兼容性较差，不推荐使用 -->

<!-- Field标签有3个属性 -->
<!-- name属性表示输出节点名 -->
<!-- value属性表示输入节点名 -->
<!-- type只有输出时有用，针对xml有属性和文本，默认是属性，text表示文本 -->
<Mapping>
    <Field name="name" value="Topics.name"></Field>
    <Field name="topic" value="Topics" type="text"></Field>
</Mapping>
